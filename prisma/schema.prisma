generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Blog {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  postedAt  DateTime @default(now())

  @@index([title])
  @@index([content])
}
